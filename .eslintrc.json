{
  "extends": [
    "next/core-web-vitals",
    "eslint:recommended",
    "plugin:@typescript-eslint/recommended",
    "plugin:@typescript-eslint/stylistic"
  ],
  "parser": "@typescript-eslint/parser",
  "plugins": [
    "@typescript-eslint",
    "unused-imports"
  ],
  "root": true,
  "rules": {
    "block-scoped-var": [
      "warn"
    ],
    "curly": [
      "error",
      "multi-line",
      "consistent"
    ],
    "default-case-last": [
      "error"
    ],
    "no-constructor-return": [
      "error"
    ],
    "no-eval": [
      "error",
      {
        "allowIndirect": false
      }
    ],
    "no-implied-eval": [
      "error"
    ],
    "no-invalid-this": [
      "error",
      {
        "capIsConstructor": true
      }
    ],
    "no-iterator": [
      "error"
    ],
    "no-proto": [
      "error"
    ],
    "no-useless-return": [
      "error"
    ],
    "no-useless-call": [
      "error"
    ],
    "no-useless-catch": [
      "error"
    ],
    "no-useless-concat": [
      "error"
    ],
    "no-useless-escape": [
      "error"
    ],
    "arrow-parens": [
      "error",
      "always",
      {
        "requireForBlockBody": false
      }
    ],
    "arrow-body-style": [
      "error"
    ],
    "constructor-super": [
      "error"
    ],
    "arrow-spacing": [
      "error",
      {
        "before": true,
        "after": true
      }
    ],
    "generator-star-spacing": [
      "error"
    ],
    "no-confusing-arrow": [
      "error",
      {
        "allowParens": true
      }
    ],
    "no-const-assign": [
      "error"
    ],
    "no-dupe-class-members": [
      "error"
    ],
    "no-duplicate-imports": [
      "error",
      {
        "includeExports": true
      }
    ],
    "no-this-before-super": [
      "error"
    ],
    "no-useless-computed-key": [
      "error",
      {
        "enforceForClassMembers": false
      }
    ],
    "no-useless-constructor": [
      "error"
    ],
    "no-console": [
      "warn"
    ],
    "no-debugger": [
      "warn"
    ],
    "array-bracket-spacing": [
      "error",
      "always",
      {
        "singleValue": true,
        "objectsInArrays": true,
        "arraysInArrays": true
      }
    ],
    "brace-style": [
      "error",
      "1tbs",
      {
        "allowSingleLine": true
      }
    ],
    "camelcase": [
      "error",
      {
        "ignoreDestructuring": false,
        "ignoreImports": true,
        "ignoreGlobals": true
      }
    ],
    "comma-dangle": [
      "error",
      "always-multiline"
    ],
    "comma-spacing": [
      "error",
      {
        "after": true
      }
    ],
    "comma-style": [
      "error",
      "last"
    ],
    "consistent-this": [
      "off",
      []
    ],
    "eol-last": [
      "error",
      "always"
    ],
    "indent": [
      "error",
      4,
      {
        "SwitchCase": 1
      }
    ],
    "max-depth": [
      "off"
    ],
    "max-len": [
      "warn",
      {
        "code": 120
      }
    ],
    "semi": [
      "error"
    ],
    "quotes": [
      "error"
    ],
    "react-hooks/exhaustive-deps": [
      "off"
    ],
    "object-curly-spacing": [
      "off"
    ],
    "consistent-return": [
      "error",
      {
        "treatUndefinedAsUnspecified": false
      }
    ],
    "default-case": [
      "error"
    ],
    "no-alert": [
      "warn"
    ],
    "no-empty-function": [
      "off",
      {
        "allow": null
      }
    ],
    "no-fallthrough": [
      "warn",
      {
        "commentPattern": ""
      }
    ],
    "no-script-url": [
      "warn"
    ],
    "no-with": [
      "error"
    ],
    "wrap-iife": [
      "off",
      null,
      {
        "functionPrototypeMethods": false
      }
    ],
    "template-curly-spacing": [
      "error",
      "never"
    ],
    "rest-spread-spacing": [
      "error",
      "never"
    ],
    "no-unused-vars": [
      "off"
    ],
    "@typescript-eslint/no-unused-vars": [
      "off"
    ],
    "unused-imports/no-unused-imports": "error",
    "unused-imports/no-unused-vars": [
      "warn",
      {
        "vars": "all",
        "varsIgnorePattern": "^_",
        "args": "after-used",
        "argsIgnorePattern": "^_"
      }
    ],
    "@typescript-eslint/object-curly-spacing": [
      "error",
      "always"
    ],
    "func-call-spacing": [
      "off"
    ],
    "@typescript-eslint/func-call-spacing": [
      "error"
    ],
    "@typescript-eslint/prefer-nullish-coalescing": [
      "off"
    ],
    "@typescript-eslint/ban-ts-comment": [
      "off"
    ],
    "@typescript-eslint/no-explicit-any": [
      "off"
    ]
  }
}
